generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id          String   @id @default(uuid())
  userId      String   
  name        String
  price       Float
  startDate   DateTime
  nextPayment DateTime
  cycle       String
  category    String   @default("Other") 
  currency    String   @default("EUR")
  createdAt   DateTime @default(now())
}